rception":{"rank":3,"characteristic":"Cunning","groupskill":false,"careerskill":false,"type":"General","max":6,"label":"Perception"},"Piloting: Planetary":{"rank":0,"characteristic":"Agility","groupskill":false,"careerskill":false,"type":"General","max":6,"label":"Piloting: Planetary"},"Piloting: Space":{"rank":0,"characteristic":"Agility","groupskill":false,"careerskill":false,"type":"General","max":6,"label":"Piloting: Space"},"Resilience":{"rank":0,"characteristic":"Brawn","groupskill":false,"careerskill":false,"type":"General","max":6,"label":"Resilience"},"Skulduggery":{"rank":0,"characteristic":"Cunning","groupskill":false,"careerskill":false,"type":"General","max":6,"label":"Skulduggery"},"Stealth":{"rank":0,"characteristic":"Agility","groupskill":false,"careerskill":false,"type":"General","max":6,"label":"Stealth"},"Streetwise":{"rank":0,"characteristic":"Cunning","groupskill":false,"careerskill":false,"type":"General","max":6,"label":"Streetwise"},"Survival":{"rank":0,"characteristic":"Cunning","groupskill":false,"careerskill":false,"type":"General","max":6,"label":"Survival"},"Vigilance":{"rank":4,"characteristic":"Willpower","groupskill":false,"careerskill":false,"type":"General","max":6,"label":"Vigilance"},"Knowledge: Core Worlds":{"rank":0,"characteristic":"Intellect","groupskill":false,"careerskill":false,"type":"Knowledge","max":6,"label":"Knowledge: Core Worlds"},"Knowledge: Education":{"rank":0,"characteristic":"Intellect","groupskill":false,"careerskill":false,"type":"Knowledge","max":6,"label":"Knowledge: Education"},"Knowledge: Lore":{"rank":0,"characteristic":"Intellect","groupskill":false,"careerskill":false,"type":"Knowledge","max":6,"label":"Knowledge: Lore"},"Knowledge: Outer Rim":{"rank":0,"characteristic":"Intellect","groupskill":false,"careerskill":false,"type":"Knowledge","max":6,"label":"Knowledge: Outer Rim"},"Knowledge: Underworld":{"rank":0,"characteristic":"Intellect","groupskill":false,"careerskill":false,"type":"Knowledge","max":6,"label":"Knowledge: Underworld"},"Knowledge: Warfare":{"rank":0,"characteristic":"Intellect","groupskill":false,"careerskill":false,"type":"Knowledge","max":6,"label":"Knowledge: Warfare"},"Knowledge: Xenology":{"rank":0,"characteristic":"Intellect","groupskill":false,"careerskill":false,"type":"Knowledge","max":6,"label":"Knowledge: Xenology"}},"attributes":{},"modifiers":{"Characteristic":{},"Skill Rank":{},"Stat":{}},"encumbrance":{"value":0,"type":"Number","label":"Encumbrance","abrev":"Encum"},"obligation":{"value":0,"type":"Number","label":"Obligation"},"duty":{"value":0,"type":"Number","label":"Duty"},"morality":{"value":0,"type":"Number","label":"Morality"},"conflict":{"value":0,"type":"Number","label":"Conflict"},"experience":{"total":0,"available":0}},"sort":100001,"flags":{"loaded":false,"config":{}},"img":"modules/swrpg/tokens/Characters/Human_M_Red_Guard.png","token":{"flags":{},"name":"test","displayName":0,"img":"modules/swrpg/tokens/Characters/Human_M_Red_Guard.png","tint":null,"width":1,"height":1,"scale":1,"lockRotation":false,"rotation":0,"vision":false,"dimSight":0,"brightSight":0,"dimLight":0,"brightLight":0,"sightAngle":360,"lightAngle":360,"lightAlpha":1,"actorId":"mmGMTFfgjZSZZ0h3","actorLink":false,"actorData":{},"disposition":-1,"displayBars":0,"bar1":{},"bar2":{},"randomImg":false},"items":[{"_id":"LnqiyF7SN0LLZ1Yn","flags":{"importid":"REFCORTSTAFF"},"name":"Refined Cortosis Staff","type":"weapon","img":"icons/svg/mystery-man.svg","data":{"description":"\n      [H3]Refined Cortosis Staff[h3]\n\nFrom the basic, utilitarian staff of a nerf herder to high-tech weapons used to subdue unruly mobs, staffs are ubiquitous throughout the galaxy. However, what would normally be a simple and common tool can be modified into a uniquely effective weapon against Force users through the use of refined cortosis. In most forms. cortosis ore resists and absorbs energy, allowing items to withstand blows from weapons that would normally shatter them. However, specifically refined cortosis has a unique secondary effect: it can short out lightsaber blades on contact, Working with this refined cortosis is extremely difficult due to the properties of the ore, and even making a simple weapon like a staff is inordinately expensive. However, such weapons can prove a deadly advantage if someone has to face down a lightsaber-wielding enemy.\n\n[P]When a character armed with a lightsaber makes a combat check targeting an opponent armed with a refined cortosis staff, [TH][TH][TH] or [DE] can be spent to cause the lightsaber to short out and deactivate after the combat check has been resolved. When a character armed with a refined cortosis staff makes a combat check targeting a character armed with a lightsaber, the attacking character may spend [AD][AD][AD] or [TR] to cause the lightsaber to short out and deactivate after the combat check has been resolved. The lightsaber may be reactivated as an incidental, but it may not be reactivated until after the last Initiative slot during the next round. Cortosis staffs require two hands to use. \n\n[BR][B]Models Include:[b] Various models. ","quantity":{"value":1,"type":"Number","label":"Quantity","abrev":"Qty"},"encumbrance":{"value":"4","type":"Number","label":"Encumbrance","abrev":"Encum"},"price":{"value":"2500","type":"Number","label":"Price"},"rarity":{"value":"7","type":"Number","label":"Rarity"},"attributes":{"attr1":{"isCheckbox":false,"mod":"damage","modtype":"Weapon Stat","value":"3"}},"hardpoints":{"value":"2","type":"Number","label":"Hard Points","abrev":"HP"},"equippable":{"value":true,"type":"Boolean","equipped":true},"skill":{"value":"Melee","type":"String","label":"Skill"},"damage":{"value":5,"type":"Number","label":"Damage","abrev":"Dam"},"crit":{"value":"5","type":"Number","label":"Critical Rating","abrev":"Crit"},"range":{"value":"Engaged","type":"String","label":"SWFFG.WeaponRangeEngaged"},"special":{"value":"","type":"String","label":"Special"}},"sort":100000},{"_id":"dRYX0nzz4CrNahRQ","flags":{"importid":"BR219"},"name":"BR-219 Heavy Blaster Pistol","type":"weapon","img":"worlds/wildspace/images/packs/oggdudeweapons/WeaponBR219.png","data":{"description":"\n      [H3]BR-219 Heavy Blaster Pistol[h3]\nThe BR-219 is a rare, high performance heavy blaster pistol produced by BlasTech. A large, intimidating weapon, the BR-219 has a short, thick barrel with a broad aperture, a heavy-duty frame, and a custom grip with an integral recoil compensation system. These super-heavy pistols were developed in response to requests from bounty hunters and law enforcement agencies for a powerful, pistol-sized weapon for use in taking down particularly dangerous opponents. To meet the demand, BlasTech turned to one of its most popular and well-respected product lines and developed the BR-219 from its E-11 series of millitary blaster rifles.\n[P]Using the E-11's reciever and firing mechanism, BlasTech engineers then reengineered the XCiter and actuating module to produce an overcharged bolt, equipped the drastically shortened barrel with an improved Galven pattern, and developed a special and highly volatile blend of blaster gases to feed their new weapon. All of this enhanced technology combined with the new bespoke blaster gas made for an extremely powerful—if slightly inaccurate and finicky—pistol. The bolt from the BR-219 is nearly as powerful as the E-11, and the weapon produces a tremendous noise and muzzle flash when fired. It is neither a subtle nor a restrained weapon; openly carrying one of these massive pistols makes an eloquent point about the user's intentions.\n[P]The BR-219 is not sold through BlasTech's usual dealer network. Instead, the weapon must be custom ordered straight from the factory. A thorough Imperial Security Bureau background check and a specially issued carry permit are required to purchase one of these weapons legally. Of course, this background check can be circumvented by purchasing the pistol on the black market or acuiring it through other less-than-legal means. Carrying a BR-219 without the proper permit is a risky business, and being found with an illegally purchased model is grounds for immediate Imperial imprisonment. Using a BR-219 in combat for an extended period of time is,  to the least, quite hazardous. The immense heat generated by the high-performance blaster gas and the overcharged action of the firing mechanism put an incredible amount of strain on the weapon.\n[P]GMs may spend [TH][TH] from a check to cause the BR-219 to become damaged one step (undamaged to minot, minor to moderate, etc.) and suffer the penalties listed on page 166 of the [B]Force and Destiny[b] Core Rulebook until repaired. In addition, Game Masters may spend [DE] to cause the BR-219 to melt down and explode, destroying the weapon and dealing the weapon's base damage to the user.","quantity":{"value":1,"type":"Number","label":"Quantity","abrev":"Qty"},"encumbrance":{"value":"2","type":"Number","label":"Encumbrance","abrev":"Encum"},"price":{"value":"625","type":"Number","label":"Price"},"rarity":{"value":"7","type":"Number","label":"Rarity"},"attributes":{},"hardpoints":{"value":"2","type":"Number","label":"Hard Points","abrev":"HP"},"equippable":{"value":true,"type":"Boolean","equipped":true},"skill":{"value":"Ranged: Light","type":"String","label":"Skill"},"damage":{"value":"8","type":"Number","label":"Damage","abrev":"Dam"},"crit":{"value":"3","type":"Number","label":"Critical Rating","abrev":"Crit"},"range":{"value":"Short","type":"String","label":"SWFFG.WeaponRangeShort"},"special":{"value":"STUNSETTING ,VICIOUS 2","type":"String","label":"Special"}},"sort":200000},{"_id":"doVCg5vZdwdgT6j8","flags":{"importid":"HBA"},"name":"Heavy Battle Armor","type":"armour","img":"icons/svg/mystery-man.svg","data":{"description":"\n      [H3]Heavy Battle Armor[h3]\n\nCertain heavy stormtroopers, bounty hunters, and even droids are fitted with heavy armor. Ostensibly, heavy armor protects against most normal blaster shots, but in the arms race of offense versus defense, the offensive power of a good blaster is still far ahead of most anything a humanoid can wear. \n\n[P]Heavy battle armor can be sealed and provide the same benefits as full-body laminate armor as above, or it can take the form of a heavy vest and blast helmet.\n[BR][B]Models Include:[b] Mandalorian Shock Trooper Armor. ","quantity":{"value":1,"type":"Number","label":"Quantity","abrev":"Qty"},"encumbrance":{"value":"6","type":"Number","label":"Encumbrance","abrev":"Encum"},"price":{"value":"5000","type":"Number","label":"Price"},"rarity":{"value":"7","type":"Number","label":"Rarity"},"attributes":{},"hardpoints":{"value":"4","type":"Number","label":"Hard Points","abrev":"HP"},"equippable":{"value":true,"type":"Boolean","equipped":true},"defence":{"value":"1","type":"Number","label":"Defence","abrev":"Def"},"soak":{"value":"2","type":"Number","label":"Soak"}},"sort":300000},{"_id":"4jCknRW4nPg9T4hq","flags":{"importid":"ADV","loaded":false},"name":"Adversary","type":"talent","img":"icons/svg/mystery-man.svg","data":{"description":"Upgrade the difficulty of any combat check targeting this character once per rank of Adversary.","quantity":{"value":1,"type":"Number","label":"Quantity","abrev":"Qty"},"encumbrance":{"value":0,"type":"Number","label":"Encumbrance","abrev":"Encum"},"price":{"value":0,"type":"Number","label":"Price"},"rarity":{"value":0,"type":"Number","label":"Rarity"},"attributes":{},"activation":{"value":"Passive","type":"String","label":"SWFFG.TalentActivationsPassive"},"ranks":{"ranked":true,"current":2,"min":0},"isForceTalent":false,"trees":[]},"sort":400000},{"_id":"agbid5bjQLjgj9z2","flags":{"importid":"HEIGHT"},"name":"Heightened Awareness","type":"talent","img":"icons/svg/mystery-man.svg","data":{"description":"Allies within close range add [BO] to Perception or Vigilance checks. Engaged allies add [BO][BO].","quantity":{"value":1,"type":"Number","label":"Quantity","abrev":"Qty"},"encumbrance":{"value":0,"type":"Number","label":"Encumbrance","abrev":"Encum"},"price":{"value":0,"type":"Number","label":"Price"},"rarity":{"value":0,"type":"Number","label":"Rarity"},"attributes":{},"activation":{"value":"Passive","type":"String","label":"SWFFG.TalentActivationsPassive"},"ranks":{"ranked":false,"current":1,"min":0},"isForceTalent":false,"trees":[]},"sort":500000},{"_id":"6HIYT6yPuWjAIBOF","flags":{"importid":"PARRY","loaded":false},"name":"Parry","type":"talent","img":"icons/svg/mystery-man.svg","data":{"description":"When hit by a melee attack, suffer 3 strain to reduce damage by 2 plus ranks in Parry.","quantity":{"value":1,"type":"Number","label":"Quantity","abrev":"Qty"},"encumbrance":{"value":0,"type":"Number","label":"Encumbrance","abrev":"Encum"},"price":{"value":0,"type":"Number","label":"Price"},"rarity":{"value":0,"type":"Number","label":"Rarity"},"attributes":{},"activation":{"value":"Active (Incidental, Out of Turn)","type":"String","label":"SWFFG.TalentActivationsActiveIncidentalOutofTurn"},"ranks":{"ranked":true,"current":4,"min":0},"isForceTalent":false,"trees":[]},"sort":600000}]}
